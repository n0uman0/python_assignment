# No 5.
def removeDuplicate(the_list): 
    final_list = [] 
    for num in the_list: 
        if num not in final_list: 
            final_list.append(num) 
    return final_list 


lst = [ 3, 6, 9, 12, 3, 30, 15, 9, 45, 36, 12, 12];
print(removeDuplicate(lst))


